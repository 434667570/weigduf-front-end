.zt-post {
   display: flex;
   width: 50px;
   height: 50px;
   position: fixed;
   bottom: 30px;
   right: 30px;
   background-color: #a09fed;
   border-radius: 50%;
   z-index: 998;
}

.zt-post view {
   color: #fff;
   font-size: 45px;
   text-align: center;
   line-height: 45px;
   margin: auto;
}
