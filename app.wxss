.container {
   height: 100%;
   flex-direction: column;
   align-items: center;
   justify-content: space-between;
   box-sizing: border-box;
   font-size: 16px;
}

page {
   background-color: #ffffff;
   color: #333;
   font-size: 16px;
   font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
}

.page__hd {
   padding: 40px;
}

.page__bd { 
   background-color: #f2f2f2;
   color: #333;
   font-size: 16px;
   font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
   padding-bottom: 200px;
}

.page__bd_spacing {
   padding-left: 15px;
   padding-right: 15px;
}

.page__ft {
   padding-bottom: 10px;
   text-align: center;
}

.page__title {
   text-align: left;
   font-size: 20px;
   font-weight: 400;
}

.page__desc {
   margin-top: 5px;
   color: #888;
   text-align: left;
   font-size: 14px;
}

.zt-panel-border {
   height: 1px;
   width: 100%;
   border-bottom: 1px solid #eee;
   margin: 10px 0;
}
button::after{
border:none;
}